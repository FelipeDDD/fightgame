{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "public/images/*", "use": "@vercel/static" },
    { "src": "public/walls/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/public/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/public/walls/(.*)", "dest": "/public/walls/$1" }
  ]
}