{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/public/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/public/walls/(.*)", "dest": "/public/walls/$1" }
  ]
}